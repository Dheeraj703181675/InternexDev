    <fields>
        <fullName>Aging_Group__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
            (  Days_Past_Due__c    &lt;= 0 ),
             
              &apos;Not Yet Due&apos; ,

                IF(  
                       (   Days_Past_Due__c    &lt;= 30  ),
                       &apos;0-30 Past Due&apos; ,
                         IF(  
                                   (   Days_Past_Due__c    &lt;= 60  ),
                                    &apos;31-60 Past Due&apos; ,
                        
                                     IF(  
                                                (   Days_Past_Due__c    &lt;= 90  ),
                                               &apos;61-90 Past Due&apos; ,
                                                IF(  
                                                        (   Days_Past_Due__c    &lt;= 120  ),
                                                            &apos;91-120 Past Due&apos; ,
                                                            IF(  
                                                                     (   Days_Past_Due__c    &lt;= 150  ),
                                                                          &apos;121-150 Past Due&apos; ,
                                                                          IF(  
                                                                                          (   Days_Past_Due__c    &lt;= 180  ),
                                                                                            &apos;151-180 Past Due&apos; ,
                                                                                             IF(  
                                                                                                       (   Days_Past_Due__c    &gt; 180  ),
                                                                                                       &apos;181+ Past Due&apos; , &apos;&apos;
                                                                                            

                                                                                              )

                                                                             )

                                                             )

                                               )
                                                                 
                                      )
                            )

                )

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Aging Group</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
