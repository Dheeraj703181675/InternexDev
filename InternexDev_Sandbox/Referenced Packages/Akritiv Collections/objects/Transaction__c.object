        <formula>IF( Days_Past_Due__c &lt;=0, &quot;Current&quot;,  
IF(Days_Past_Due__c &lt;=( $Setup.BucketConfig_Singlec__c.Age1__c ), 
&quot;1-&quot;+ TEXT( $Setup.BucketConfig_Singlec__c.Age1__c ) , 
IF(Days_Past_Due__c &lt;=( $Setup.BucketConfig_Singlec__c.Age2__c ), 
TEXT( $Setup.BucketConfig_Singlec__c.Age1__c+1 )+&quot;-&quot;+ TEXT( $Setup.BucketConfig_Singlec__c.Age2__c ) ,
IF(Days_Past_Due__c &lt;=( $Setup.BucketConfig_Singlec__c.Age3__c ), 
TEXT( $Setup.BucketConfig_Singlec__c.Age2__c+1 )+&quot;-&quot;+ TEXT( $Setup.BucketConfig_Singlec__c.Age3__c ) , 
IF(Days_Past_Due__c &lt;=( $Setup.BucketConfig_Singlec__c.Age4__c ), 
TEXT( $Setup.BucketConfig_Singlec__c.Age3__c+1 )+ &quot;-&quot;+TEXT( $Setup.BucketConfig_Singlec__c.Age4__c ), 
&quot;Over &quot; +TEXT( $Setup.BucketConfig_Singlec__c.Age4__c )
)))))</formula>
