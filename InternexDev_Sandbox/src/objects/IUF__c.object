    <validationRules>
        <fullName>Validation_on_Due_Recovered_and_IUF</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c,&apos;Manual&apos;)&amp;&amp;IUF_Retained__c &gt; 0 &amp;&amp; (Interest_Due_Recovered__c &gt; 0 || UL_Fee_Due_Recovered__c &gt; 0)&amp;&amp; IUF_Retained__c != (Interest_Due_Recovered__c +  UL_Fee_Due_Recovered__c)</errorConditionFormula>
        <errorMessage>Retained Amount must be equal to Sum of Interest Due Recovered and UL Fee Due Recovered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_IUF</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c,&apos;Manual&apos;)&amp;&amp; IF(UL_Fee_Due_Recovered__c==0&amp;&amp; Interest_Due_Recovered__c==0,IUF_Retained__c &gt; Client_Account__r.Additional_IUF_Required__c,IUF_Retained__c &gt; (Client_Account__r.Additional_IUF_Required__c +   BLANKVALUE(Client_Account__r.Interest_Due__c,0)+  BLANKVALUE(Client_Account__r.UL_Fee_Due__c,0)))</errorConditionFormula>
        <errorMessage>Retained amount should be less than the Client-Account IUF Reserve Shortfall.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Negative_number</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c,&apos;Manual&apos;) &amp;&amp;  (IUF_Retained__c &lt; 0 ||  UL_Fee_Due_Recovered__c &lt;0 || IUF_Retained__c &lt; 0)</errorConditionFormula>
        <errorMessage>Please enter the Positive Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Update</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNEW())&amp;&amp;ISPICKVAL(Type__c,&apos;Manual&apos;)&amp;&amp;((PRIORVALUE(IUF_Retained__c)!=  IUF_Retained__c)||(PRIORVALUE(Interest_Due_Recovered__c)!=  Interest_Due_Recovered__c)||(PRIORVALUE(UL_Fee_Due_Recovered__c)!= UL_Fee_Due_Recovered__c))</errorConditionFormula>
        <errorMessage>You can not modify the existing record.Please create the new IUF.</errorMessage>
    </validationRules>
