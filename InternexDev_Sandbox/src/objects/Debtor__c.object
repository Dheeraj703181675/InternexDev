        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var Status = &quot;{!Debtor__c.Debtor_Status__c}&quot;;
var duns=  &quot;{!Account.Duns_Number__c}&quot;;

if(Status.toUpperCase() == &apos;PENDING VERIFICATION&apos;)
{
if(duns == null || duns ==&apos;&apos;)
{
alert(&apos;Duns number is required&apos;);
}
else
{
// identify the record 
var o = new sforce.SObject(&quot;Debtor__c&quot;);
 
o.id = &quot;{!Debtor__c.Id}&quot;; 

// make the field change 
o.Debtor_Status__c = &quot;Pending&quot;; 

// save the change 
sforce.connection.update([o]); 

