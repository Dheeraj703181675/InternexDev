    <fields>
        <fullName>Actual_Concentration__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(ISNULL(Concentration_Limit__c),ISBLANK(Concentration_Limit__c)),IF(Total_Eligible_AR__c==0,0,(Eligible_AR__c/Total_Eligible_AR__c)* Client_Account__r.Eligible_For_Funding_formula__c), Concentration_Limit__c * Client_Account__r.Eligible_For_Funding_formula__c)</formula>
        <inlineHelpText>Formula: Eligible for Funding of Client-Account * Concentration Limit</inlineHelpText>
        <label>Max Concentration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
