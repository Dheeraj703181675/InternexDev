        <fullName>Back_out_Webcash_Batch</fullName>
        <availability>online</availability>
        <description>Back-out Webcash Batch</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Back-out Webcash Batch</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;

var records = {!GETRECORDIDS($ObjectType.WebCash_Load_Batch__c)};


if (records[0] == null) {
	alert(&apos;No record is selected &apos;);
}
else {
	if(records.length &gt;1){
		alert(&apos;Please select one record only&apos;);
	}else{


	result = sforce.connection.query(&quot;Select Id, IsBackOut__c from WebCash_Load_Batch__c where  Id=&apos;&quot;+records[0]+&quot;&apos;&quot;);

       
	records1 = result.getArray(&quot;records&quot;);
	if(records1.length==1)
	{
		
		var record = records1[0];
		if(record.IsBackOut__c == &quot;true&quot;){
			alert(&apos;Record already Back-out&apos;);
		}else{
		
			//var loc = window.location.href ;
			var url = &apos;/apex/BackOutPage?id=&apos;+records[0];
			var left = (screen.width)-650;
			var top = (screen.height)-450;
			if(confirm(&quot;This action would result in entire selected batch being backed-out. Are you sure?&quot;))
			{			
				window.open (url ,&quot;BackOut&quot;,&quot;toolbar=0,location=0, directories=0, status=0, menubar=0, scrollbars=1,resizable=no , closable = 0 ,width=500,height=200&quot;);
			}
		}		
		
	}
//


		
	}

}</url>
