    <validationRules>
        <fullName>Lock_Box_State_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(State__c)),OR(LEN(State__c)&gt;2 ,OR(LEN(SUBSTITUTE(State__c,&apos;,&apos;,NULL)) != 2,IF(FIND(SUBSTITUTE(State__c,&apos;,&apos;,NULL), &apos;AL,AK,AZ,AR,CA,CO,CT,DE,DC,FL,GA,HI,ID,IL,IN,IA,KS,KY,LA,ME,MD,MA,MI,MN,MS,MO,MT,NE,NV,NH,NJ,NM,NY,NC,ND,OH,OK,OR,PA,RI,SC,SD,TN,TX,UT,VT,VA,WA,WV,WI,WY&apos;) != 0,False,True))))</errorConditionFormula>
        <errorDisplayField>State__c</errorDisplayField>
        <errorMessage>Please enter the valid USA ISO state codes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Box_Zipi_code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Zip_Code__c)),OR(AND(Not(LEN(Zip_Code__c) = 5),Not(LEN(Zip_Code__c) = 10)),AND(LEN(Zip_Code__c) = 10,NOT(REGEX( Zip_Code__c, &quot;[0-9]{5}-[0-9]{4}&quot;))),AND(LEN(Zip_Code__c) = 5,NOT(REGEX( Zip_Code__c, &quot;[0-9]{5}&quot;)))))</errorConditionFormula>
        <errorDisplayField>Zip_Code__c</errorDisplayField>
        <errorMessage>Please enter the Zip Code in the format &quot;99999-9999&quot; or &quot;99999&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Lockbox_for_one_ClientAccount_rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),Client_Account__r.Lockbox_Accounts_count__c &gt; 0)</errorConditionFormula>
        <errorMessage>You cannot insert multiple Lock-box Accounts for same Client-account.</errorMessage>
    </validationRules>
